pipeline {
    agent any
     stages 
      stage ('vcs') {
        steps {
            git branch: 'master' ,
             url:'https://github.com/madhu0018/terraformpractice.git'
        }
      }
        stage ('provisioner')
          steps {
                 sh "terraform apply -var-file='dev.tfvars' -aout -approve"
        }
  }
    